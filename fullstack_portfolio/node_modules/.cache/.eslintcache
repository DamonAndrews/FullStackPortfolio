ange}\r\n                />\r\n                <input\r\n                  className=\"form-input\"\r\n                  placeholder=\"Password\"\r\n                  name=\"password\"\r\n                  type=\"password\"\r\n                  value={formState.password}\r\n                  onChange={handleChange}\r\n                />\r\n                <button\r\n                  className=\"btn btn-block btn-primary\"\r\n                  style={signUpPage.submitButton}\r\n                  type=\"submit\"\r\n                >\r\n                  <b>SUBMIT</b>\r\n                </button>\r\n              </form>\r\n            )}\r\n\r\n            {error && (\r\n              <div className=\"my-3 p-3 bg-secondary text-white\">\r\n                {error.message}\r\n              </div>\r\n            )}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </main>\r\n  );\r\n};\r\n\r\nexport default Signup;\r\n","C:\\Users\\damon\\OneDrive\\Desktop\\All_Projects\\FullStackPortfolio\\fullstack_portfolio\\src\\pages\\SingleThought.js",[],[],"C:\\Users\\damon\\OneDrive\\Desktop\\All_Projects\\FullStackPortfolio\\fullstack_portfolio\\src\\pages\\Login.js",[],[],"C:\\Users\\damon\\OneDrive\\Desktop\\All_Projects\\FullStackPortfolio\\fullstack_portfolio\\src\\pages\\Portfolio.js",[],[],"C:\\Users\\damon\\OneDrive\\Desktop\\All_Projects\\FullStackPortfolio\\fullstack_portfolio\\src\\utils\\queries.js",[],[],"C:\\Users\\damon\\OneDrive\\Desktop\\All_Projects\\FullStackPortfolio\\fullstack_portfolio\\src\\utils\\auth.js",[],[],"C:\\Users\\damon\\OneDrive\\Desktop\\All_Projects\\FullStackPortfolio\\fullstack_portfolio\\src\\utils\\mutations.js",[],[],"C:\\Users\\damon\\OneDrive\\Desktop\\All_Projects\\FullStackPortfolio\\fullstack_portfolio\\src\\components\\CommentForm\\index.js",["83"],[],"import React, { useState } from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport { useMutation } from '@apollo/client';\r\n\r\nimport { ADD_COMMENT } from '..